// Unlimited precision decimal tests - using 'N' suffix
// These test the new DECIMAL_BIG type with unlimited precision

"===== BASIC UNLIMITED DECIMAL LITERALS ====="
3.14159N
2.71828N
0.0N
-1.5N
123456789.987654321N

"===== HIGH PRECISION DECIMAL LITERALS ====="
// More than 38 digits - should preserve full precision
3.14159265358979323846264338327950288419N
2.71828182845904523536028747135266249775N

"===== UNLIMITED DECIMAL ADDITION ====="
3.14159N + 2.71828N
3.14159265358979323846264338327950288419N + 2.71828182845904523536028747135266249775N
0.0N + (-1.5N)
123456789.987654321N + 3.14159N

"===== MIXED PRECISION (fixed + unlimited â†’ unlimited) ====="
3.14159n + 2.71828N
3.14159N + 2.71828n
5n + 10N
10N + 5n

"===== UNLIMITED DECIMAL SUBTRACTION ====="
3.14159N - 2.71828N
3.14159265358979323846264338327950288419N - 2.71828182845904523536028747135266249775N
2.71828N - 3.14159N
0.0N - (-1.5N)

"===== UNLIMITED DECIMAL MULTIPLICATION ====="
3.14159N * 2.71828N
3.14159265358979323846264338327950288419N * 2.71828182845904523536028747135266249775N
(-1.5N) * (-1.5N)
0.0N * 123456789.987654321N

"===== UNLIMITED DECIMAL DIVISION ====="
3.14159N / 2.71828N
1N / 3N
10N / 3N
3.14159265358979323846264338327950288419N / 2.71828182845904523536028747135266249775N
(-1.5N) / (-1.5N)

"===== UNLIMITED DECIMAL MODULO ====="
7.5N % 2.3N
3.14159N % 1.0N
10N % 3N

"===== UNLIMITED DECIMAL POWER ====="
2.71828N ** 2
3.14159N ** 0.5
2N ** 10

"===== MIXED WITH INTEGER ====="
3.14159N + 5
10 + 2.71828N
3.14159N * 2
3 * (-1.5N)
3.14159N / 2
10 / 2.71828N

"===== MIXED WITH FLOAT ====="
3.14159N + 1.5
2.5 + 2.71828N
3.14159N * 0.5
2.0 * 2.71828N
3.14159N / 2.0
6.28318 / 3.14159N

"===== VARIABLES WITH UNLIMITED DECIMAL ====="
let pi_big = 3.14159265358979323846264338327950288419N
let e_big = 2.71828182845904523536028747135266249775N
pi_big
e_big
pi_big + e_big
pi_big * e_big
pi_big / e_big

"===== ARRAYS WITH UNLIMITED DECIMAL ====="
[1.5N, 2.75N, 3.14159265358979323846264338327950288419N]

"===== COMPARISON: FIXED vs UNLIMITED ====="
// These should behave the same way mathematically
3.14159n + 2.71828n   // fixed (38 digit)
3.14159N + 2.71828N   // unlimited

"===== END OF UNLIMITED DECIMAL TESTS ====="
