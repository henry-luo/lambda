{
    "output": "lambda_core.wasm",
    "source_dirs": [
        "lambda/input",
        "lambda/format"
    ],
    "source_files": [
        "lib/strbuf.c",
        "lib/strview.c",
        "lib/arraylist.c",
        "lib/file.c",
        "lib/hashmap.c",
        "lib/utf.c"
    ],
    "libraries": [
        {
            "name": "wasm-stubs",
            "include": "wasm-deps/include"
        },
        {
            "name": "tree-sitter",
            "include": "lambda/tree-sitter/lib/include",
            "lib": "lambda/tree-sitter/libtree-sitter.a",
            "link": "static"
        },
        {
            "name": "tree-sitter-lambda",
            "include": "lambda/tree-sitter-lambda/bindings/c",
            "lib": "lambda/tree-sitter-lambda/libtree-sitter-lambda.a",
            "link": "static"
        }
    ],
    "warnings": [
        "format"
    ],
    "flags": [
        "fms-extensions",
        "O2",
        "DWASM_BUILD=1",
        "DCROSS_COMPILE=1",
        "D_POSIX_C_SOURCE=200809L"
    ],
    "linker_flags": [
        "Wl,--no-entry",
        "Wl,--export-all",
        "Wl,--allow-undefined",
        "nostdlib"
    ],
    "debug": false,
    "build_dir": "build_wasm_core",
    "cross_compile": true,
    "target_triplet": "wasm32-wasi"
}
