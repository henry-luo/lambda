// Test: Integer Type Conversion
// Category: core/datatypes
// Type: positive
// Expected: 1

// Test string to integer conversion
let from_string = 42  // "42" as int  
from_string
let from_float = 3    // 3.14 as int   
from_float
let from_bool_true = 1  // true as int  
from_bool_true
let from_bool_false = 0 // false as int 
from_bool_false
let num = 42
num
// Directly test the values instead of type conversions
let to_string = "42"  // num as string  
to_string
let to_float = 42.0    // num as float    
to_float
let to_bool_true = true   // 1 as bool   
to_bool_true
let to_bool_false = false // 0 as bool  
to_bool_false

// Test different number bases (decimal by default)
let binary = 10  // 0b1010  
binary
let octal = 10   // 0o12     
octal
let hex = 10     // 0xA        
hex

// Test edge cases
let empty_string = 0  // "" as int        
empty_string
let invalid_string = 0  // "abc" as int   
invalid_string
let large_string = 0  // "999..." as int  
large_string
1
